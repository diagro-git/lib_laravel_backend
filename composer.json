{
    "name": "diagro/lib_laravel_backend",
    "description": "Library for backends written in Laravel",
    "type": "library",
    "version": "10.0.0",
    "license": "Beerware",
    "authors": [
        {
            "name": "Stijn Leenknegt",
            "email": "stijn@diagro.be"
        }
    ],
    "require": {
        "php": "^8.1",
        "laravel/framework":"^10.0",
        "diagro/lib_laravel_token": "^10.0.0",
        "diagro/lib_laravel_api": "^10.0.0",
        "guzzlehttp/guzzle": "^7.9.3",
        "hedii/laravel-gelf-logger": "8.1.0",
        "graylog2/gelf-php": "2.0.2"
    },
    "autoload": {
        "psr-4": {
            "Diagro\\Backend\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Diagro\\Backend\\DiagroServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
